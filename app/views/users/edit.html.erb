<%= form_with(model: current_user, local: true) do |f| %>
  <% if current_user.image? %>
    <%= image_tag current_user.image.to_s %>
  <% else %>
    <%= image_tag("/assets/default.jpg") %>
  <% end %>
  <%#= f.hidden_field :image_cache %> 
  <%= f.file_field :image %>
  <%= f.submit 'ユーザー画像を登録する', class: 'btn btn-primary' %>
<% end %>
<%= link_to 'Myページに戻る', user_path %> 


