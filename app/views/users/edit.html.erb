<%= form_with(model: @user, local: true) do |f| %>
  <% if @user.image? %>
    <%= image_tag @user.image.url %>
  <% else %>
    <%= image_tag("/images/icon/no-image.png") %>
  <% end %>
  <%= f.file_field :image %>
  <%= f.submit 'ユーザー画像を登録する', class: 'btn btn-primary' %>
<% end %>
<%= link_to 'Myページに戻る', user_path %> 

